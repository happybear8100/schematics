{
  "name": "@egervari/schematics-angular",
  "version": "0.0.0",
  "description": "A blank schematics",
  "scripts": {
    "build:schematics": "tsc -p tsconfig.json --outDir ./dist",
    "copy:schematics": "cpr src dist/schematics --deleteFirst",
    "build:lib": "ng-packagr -p package.json && npm run build:schematics && npm run copy:schematics",
    "test": "npm run build:schematics && jasmine src/**/*.spec.js",
    "pretty:check": "prettier --list-different \"src/**/*.ts\"",
    "pretty:fix": "prettier --write \"src/**/*.ts\"",
    "lint:check": "node_modules/.bin/tslint --project ./",
    "lint:fix": "node_modules/.bin/tslint --project ./ --fix",
    "check": "npm run pretty:check && npm run lint:check",
    "fix": "npm run pretty:fix && npm run lint:fix",
    "publish": "npm publish --access public"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "devDependencies": {
    "@angular-devkit/core": "^0.8.5",
    "@angular-devkit/schematics": "^0.8.5",
    "@angular/compiler": "^6.1.10",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@schematics/angular": "^0.8.5",
    "@types/jasmine": "^2.6.0",
    "@types/node": "^8.0.31",
    "cpr": "^3.0.1",
    "jasmine": "^2.8.0",
    "ng-packagr": "^4.3.1",
    "prettier": "^1.14.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^2.5.2"
  }
}
