{
  "name": "angular-feature-schematic",
  "version": "0.0.0",
  "description": "A blank schematics",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "pretty:check": "prettier --list-different \"src/**/*.ts\"",
    "pretty:fix": "prettier --write \"src/**/*.ts\"",
    "lint:check": "node_modules/.bin/tslint --project ./",
    "lint:fix": "node_modules/.bin/tslint --project ./ --fix",
    "check": "npm run pretty:check && npm run lint:check",
    "fix": "npm run pretty:fix && npm run lint:fix",
    "start": "npm run build && schematics .:feature --name=login --path=src/app/modules"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^0.8.5",
    "@angular-devkit/schematics": "^0.8.5",
    "@types/jasmine": "^2.6.0",
    "@types/node": "^8.0.31",
    "jasmine": "^2.8.0",
    "typescript": "^2.5.2"
  },
  "devDependencies": {
    "prettier": "^1.14.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0"
  }
}
